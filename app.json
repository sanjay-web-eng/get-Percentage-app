{
  "expo": {
    "name": "Percentage app",
    "slug": "Percentage app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-of -logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-of -logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sanjayop.yo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-of -logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-of -logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1c037656-4a39-448a-9a97-3496d040d46d"
      }
    }
  }
}
